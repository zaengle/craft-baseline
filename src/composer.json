{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/aws-s3": "^2.2.1",
    "craftcms/ckeditor": "^4.0.0-beta.7",
    "craftcms/cms": "^5.0.0",
    "craftcms/feed-me": "^6.0.0",
    "craftcms/redactor": "^4.2.0",
    "diginov/craft-sentry-logger": "^5.0",
    "doublesecretagency/craft-siteswitcher": "^3.0",
    "nystudio107/craft-seomatic": "^5.1",
    "nystudio107/craft-vite": "^5.0.0-beta.3",
    "solspace/craft-freeform": "^5.9.14",
    "spicyweb/craft-embedded-assets": "^5.0",
    "topshelfcraft/environment-label": "^5.0",
    "verbb/expanded-singles": "^3.0",
    "verbb/navigation": "^3.0",
    "vlucas/phpdotenv": "^5.4.0",
    "zaengle/craft-conventions": "^5.0.0-beta.1",
    "zaengle/craft-toolbelt": "^5.0.0-beta.1"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "nystudio107/craft-autocomplete": "^1.11",
    "pestphp/pest": "^2.33.2",
    "phpstan/phpstan": "^1.7",
    "roave/security-advisories": "dev-latest",
    "symplify/easy-coding-standard": "^10.2",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "authors": [
    {
      "name": "Zaengle Corp",
      "homepage": "https://zaengle.com/"
    }
  ],
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "pestphp/pest-plugin": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix --config ../ecs.php",
    "phpstan": "vendor/bin/phpstan analyse --memory-limit=1G",
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup/welcome"
    ],
    "craft-update": [
      "@pre-craft-update",
      "@post-craft-update"
    ],
    "pre-craft-update": [],
    "post-craft-update": [
      "Composer\\Config::disableProcessTimeout",
      "@php craft install/check && php craft clear-caches/all --interactive=0 || exit 0",
      "@php craft install/check && php craft up --interactive=0 || exit 0"
    ],
    "pre-update-cmd": "@pre-craft-update",
    "pre-install-cmd": "@pre-craft-update",
    "post-update-cmd": "@post-craft-update",
    "post-install-cmd": "@post-craft-update",
    "test": "pest",
    "coverage": "pest --coverage"
  }
}
